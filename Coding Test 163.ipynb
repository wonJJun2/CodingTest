    "def solution(s):\n",
    "    answer = ''\n",
    "    new_list = s.split(' ')\n",
    "    for i in new_list:\n",
    "        for j in range(len(i)):\n",
    "            if j % 2 == 0:\n",
    "                answer += i[j].upper()\n",
    "            else:\n",
    "                answer += i[j].lower()\n",
    "        answer+= ' '\n",
    "    return answer[0:-1]"
   ]
