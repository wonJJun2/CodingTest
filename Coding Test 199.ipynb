   "source": [
    "num = int(input())\n",
    "student = []\n",
    "\n",
    "for _ in range(num):\n",
    "    w, h = map(int, input().split())\n",
    "    student.append((w, h))\n",
    "\n",
    "for i in student:\n",
    "    rank = 1\n",
    "    for j in student:\n",
    "        if i[0] < j[0] and i[1] < j[1]:\n",
    "                rank += 1\n",
    "    print(rank, end = \" \")"
   ]
  }
