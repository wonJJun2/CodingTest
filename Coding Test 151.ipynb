    "def solution(price, money, count):\n",
    "    total = 0\n",
    "    for i in range(1,count+1):\n",
    "        total += price*i\n",
    "        if total > money:\n",
    "            answer = total - money\n",
    "        elif money >= total:\n",
    "            answer = 0\n",
    "    return answer"
   ]
